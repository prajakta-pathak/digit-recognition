#include<stdio.h>
#include<stdlib.h>
#include<string.h>
int main()
{
	FILE *f0,*f1;
	f0=fopen("data.txt","r");
	f1=fopen("trainingdata.txt","w+");
	int i=0;
	char c;
	while(i!=2)
	{
		while((c=fgetc(f0))!=' ')
		{
		
			putc(c,f1);
		}
		i++;
		fseek(f0,1,SEEK_CUR);
		c=fgetc(f0);
		putc('\n',f1);
	}
	fclose(f0);
	fclose(f1);
}
